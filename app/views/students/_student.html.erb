<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Student:</strong>
    <%= student.student_id %>
  </p>

  <p>
    <strong>Class period:</strong>
    <%= student.class_period %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= student.teacher_id %>
  </p>

</div>
